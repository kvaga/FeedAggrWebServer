# This configuration file should be located in the $catalona.base/conf path
#
# The path must contain only forward slashes. This folder must contain two subfolders: feeds and users
data.path=C:/FeedAggrWebServer/data

# Credential of the admin user
admin.login=your_user
admin.password=password_of_user



#InfluxDB parameters
influxdb.enabled=false
influxdb.host=localhost
influxdb.dbname=some_db_name
influxdb.port=8086
influxdb.threads.numder=10
influxdb.attempts=5
influxdb.timeout=5000

timeout.waittime_after_get_content_url_exception_in_millis=3600000
